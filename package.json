{
  "name": "commitCheck",
  "version": "1.0.0",
  "description": "commit提交校验",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cxuhwiuefhuefu/commitCheck.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cxuhwiuefhuefu/commitCheck/issues"
  },
  "homepage": "https://github.com/cxuhwiuefhuefu/commitCheck#readme",
  "devDependencies": {
    "commitizen": "^4.2.1",
    "commitlint": "^9.1.2",
    "commitlint-config-cz": "^0.13.2",
    "conventional-changelog": "^3.1.23",
    "cz-conventional-changelog": "^3.2.1",
    "cz-customizable": "^6.3.0",
    "husky": "^4.2.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
